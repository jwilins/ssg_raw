{
	"configurations": [
		{
			"name": "MSVC 2022, 32-bit",
			"includePath": [
				"${workspaceFolder}/**"
			],
			"cppStandard": "c++23",
			"intelliSenseMode": "windows-msvc-x86",
			"defines": [
				"WIN32"
			],
			"compilerArgs": [
				"/reference", "std=${workspaceFolder}\\obj\\stdd.ifc",
				"/reference", "std.compat=${workspaceFolder}\\obj\\std.compatd.ifc"
			]
		}
	],
	"version": 4
}
